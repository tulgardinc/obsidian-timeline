{
	"name": "obsidian-timeline-plugin",
	"version": "1.0.0",
	"description": "Display timeline cards on an infinite canvas with pan/zoom capabilities",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint .",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage"
	},
	"keywords": [],
	"license": "0-BSD",
	"devDependencies": {
		"@eslint/js": "9.30.1",
		"@tsconfig/svelte": "^5.0.7",
		"@types/node": "^18.19.0",
		"@vitest/coverage-v8": "^1.6.0",
		"esbuild": "0.25.5",
		"esbuild-svelte": "^0.9.4",
		"eslint": "^9.0.0",
		"eslint-plugin-obsidianmd": "0.1.9",
		"globals": "14.0.0",
		"jiti": "2.6.1",
		"obsidian": "^1.7.2",
		"svelte": "^5.50.0",
		"svelte-preprocess": "^6.0.3",
		"tslib": "2.4.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "8.35.1",
		"vitest": "^1.6.0"
	}
}
